    "lite",
    "mock",
    "routes",
    "route variants",
    "web sockets",
    "api",
    "rest",
    "express",
    "testing",
    "development",
    "middleware"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/burt202/mocks-server-lite.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
