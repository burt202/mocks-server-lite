{
  "name": "my-mocks-server",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint --ext js,ts,tsx src/",
    "prettier-check": "prettier \"**/*.ts{,x}\" --check",
    "test": "vitest run",
    "test:watch": "vitest",
    "pre-commit": "npm run lint && npm run prettier-check",
    "pre-push": "npm run test",
    "mocks": "npx ts-node src/mocks/server.ts"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.17.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "^9.0.11",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.4.0"
  }
}
